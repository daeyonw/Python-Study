    "list_ = []\n",
    "\n",
    "for i in range(n):\n",
    "    a,b = input().split()\n",
    "    list_.append([int(a),b])\n",
    "\n",
    "for i in sorted(list_,key=lambda x:x[0]):\n",
    "    print(i[0], i[1])\n"
